{
    "name": "annke-to-alexa",
    "version": "1.0.0",
    "description": "Unified Node.js application for DVR discovery, RTSP to WebRTC conversion, and Alexa integration",
    "main": "src/index.ts",
    "type": "module",
    "scripts": {
        "dev": "bun run --watch src/index.ts",
        "start": "bun run src/index.ts",
        "build": "bun build src/index.ts --compile --outfile dist/annke-to-alexa",
        "build:windows": "bun build src/index.ts --compile --target=bun-windows-x64 --outfile dist/annke-to-alexa.exe",
        "build:linux": "bun build src/index.ts --compile --target=bun-linux-x64 --outfile dist/annke-to-alexa-linux",
        "build:all": "bun run build:windows && bun run build:linux",
        "test": "bun test",
        "lint": "eslint src/**/*.ts"
    },
    "keywords": [
        "alexa",
        "webrtc",
        "rtsp",
        "dvr",
        "onvif",
        "smart-home"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@types/node": "^25.0.3",
        "axios": "^1.6.5",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "helmet": "^7.1.0",
        "http-proxy-middleware": "^3.0.5",
        "jsonwebtoken": "^9.0.2",
        "node-onvif-ts": "^0.1.7",
        "uuid": "^9.0.1",
        "ws": "^8.16.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/ws": "^8.5.10",
        "@types/cors": "^2.8.17",
        "@types/uuid": "^9.0.7",
        "bun-types": "latest"
    }
}